{
    "name": "devhau/laravel-module",
    "description": "laravel modules",
    "keywords": [
        "devhau",
        "core"
    ],
    "homepage": "https://github.com/devhau/laravel-module",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "nguyen van hau",
            "email": "nguyenvanhaudev@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.1"
    },
    "autoload": {
        "psr-4": {
            "DevHau\\Modules\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DevHau\\Modules\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevHau\\Modules\\LaravelModulesServiceProvider"
            ]
        }
    }
}